        <script><![CDATA[try {
    var erpCaseSysId = current.sys_id;
	var jsonString = producer.multi_product_set;
	var prod_data = JSON.parse(jsonString);
	gs.addInfoMessage(prod_data);
    for (var i = 0; i < prod_data.length; i++) {
        var record = prod_data[i];
        var erpProduct = new GlideRecord('x_1166825_erp_assi_erp_product');
        erpProduct.initialize();
        erpProduct.setValue('u_reference_1', erpCaseSysId); 
        erpProduct.setValue('name', record.name); 
        erpProduct.setValue('manufacturer', record.manufacturer); 
        erpProduct.setValue('cmdb_model_category', record.cmdb_model_category); 
        erpProduct.setValue('model_number', record.model_number); 
        erpProduct.setValue('cost', record.cost); 
        erpProduct.setValue('status', record.status); 
        erpProduct.setValue('depreciation', record.depreciation); 
        erpProduct.setValue('acquisition_method', record.acquisition_method); 
        erpProduct.setValue('u_string_full_utf8_1', record.var1);
        var erpProductSysId = erpProduct.insert();
    }
    }
 catch (e) {
    gs.error('Error: ' + e.message);
    gs.addErrorMessage('Error: ' + e.message);
}






]]></script>
